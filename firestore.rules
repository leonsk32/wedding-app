rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
  	match /games/quiz {
      match /questions/{document=**} {
        allow write: if true;
        allow read: if true;
      }
      match /current/{document=**} {
        allow write: if true;
        allow read: if true;
      }
    }
  }
}
